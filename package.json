{
  "name": "pincha_express",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:dead": "ts-prune -p tsconfig.json",
    "deadcode": "ts-prune -p tsconfig.json -u -e",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:merge": "node scripts/merge-prisma-schemas.js",
    "prisma:generate": "npm run prisma:merge && prisma generate",
    "prisma:db:push": "npm run prisma:merge && prisma db push",
    "prisma:migrate": "npm run prisma:merge && prisma migrate dev --name",
    "prisma:studio": "npm run prisma:merge && prisma studio",
    "db:seed": "ts-node prisma/seed/seed.ts"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@automapper/pojos": "^8.8.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.11.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express-rate-limit": "^7.5.1",
    "form-data": "^4.0.4",
    "googleapis": "^155.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "mailgun.js": "^12.0.3",
    "nestjs-prisma": "^0.25.0",
    "nodemailer": "^7.0.5",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "redis": "^5.8.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/nodemailer-mailgun-transport": "^1.4.6",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.39.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "resolutions": {
      "lodash.merge": "4.6.2",
      "minimatch": "9.0.4",
      "scslre": "0.6.1",
      "semver": "7.5.4"
    }
  }
}
